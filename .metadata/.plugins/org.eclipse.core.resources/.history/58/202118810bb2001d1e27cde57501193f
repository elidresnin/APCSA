
public class NewWaveMain {
	
	public static String newWAveCAse(String input) {
		String output = "";
		
		for(int i = 0; i < input.length(); i++) {
			output += input.charAt(i);
			if(Character.isUpperCase(input.charAt(i)) && i < input.length() - 1) {
				output += 1 + Character.toUpperCase(input.charAt(i+1));
				i++;
			}

			
		}
		return output;
	}
	
	public static void main(String[] args) {
		
		System.out.println(newWAveCAse("newWaveComputers"));
	}
}
