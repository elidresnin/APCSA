public class Example {
	private int age;
	private String name;
	public static final int NUMBEROFFRIENDS = 0;

	public Example() {
		this(1, "bob");
	}
	public Example(int age, String name) {
		this.age = age;
		this.name = name;
	}

	public int getAge() {
		return age;
	}
	public void setAge(int age) {
		this.age = age;
	}
	public String getName() {
		return name;
	}
	public void setName(String name) {
		this.name = name;
	}
	public String toString() {
		return "Example [age=" + age + ", name=" + name + "]";
	}
	public static String personGender() {
		return "You have " + NUMBEROFFRIENDS + " friends";
	}
}
